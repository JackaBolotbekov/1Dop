<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.bottomnavigation.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_webFragment"
            app:destination="@id/webFragment" />
        <action
            android:id="@+id/action_homeFragment_to_cubeFragment2"
            app:destination="@id/cubeFragment" />
    </fragment>
    <fragment
        android:id="@+id/cubeFragment"
        android:name="com.example.bottomnavigation.fragments.CubeFragment"
        android:label="fragment_cube"
        tools:layout="@layout/fragment_cube" />
    <fragment
        android:id="@+id/webFragment"
        android:name="com.example.bottomnavigation.fragments.WebFragment"
        android:label="fragment_web"
        tools:layout="@layout/fragment_web" />
</navigation>